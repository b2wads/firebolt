{
  "name": "nodejs-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate para projetos NodeJS",
  "main": "app.js",
  "dependencies": {
    "axios": "0.19.0",
    "enzyme": "3.10.0",
    "grimorio-ui": "./packages/grimorio-ui-6.2.0.tgz",
    "immer": "^4.0.1",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-root-import": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "codecov": "3.5.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-unsafe-regex": "1.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.5.0",
    "file-loader": "4.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "jsdom": "15.1.1",
    "mini-css-extract-plugin": "0.8.0",
    "postcss-custom-properties": "^9.0.2",
    "postcss-loader": "3.0.0",
    "prettier": "1.17.1",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "supertest": "4.0.2",
    "webpack": "4.37.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "3.8.2"
  },
  "scripts": {
    "codecov": "./node_modules/.bin/codecov",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "fmt": "./node_modules/.bin/prettier --write \"./**/*.js\" && ./node_modules/.bin/eslint --fix .",
    "fmt:check": "./node_modules/.bin/prettier --check \"./**/*.js\" && ./node_modules/.bin/eslint .",
    "start:dev": "webpack-dev-server --config ./webpack/webpack.config.js",
    "prebuild": "rm -rf public",
    "build": "webpack --config ./webpack/webpack.config.js",
    "create:comp:redux": "bash ./internals/component/create-with-redux.sh ./src/components",
    "create:comp": "bash ./internals/component/create.sh ./src/components",
    "create:view": "bash ./internals/component/create-with-redux.sh ./src/views",
    "test": "./node_modules/.bin/jest --coverage",
    "test:unit": "./node_modules/.bin/jest --coverage test test/unit/*"
  },
  "repository": {
    "type": "git",
    "url": "github.com:b2wads/nodejs-boilerplate"
  },
  "keywords": [
    "nodejs",
    "boilerplate",
    "webapp",
    "worker"
  ],
  "author": "B2WADS",
  "license": "MIT"
}
